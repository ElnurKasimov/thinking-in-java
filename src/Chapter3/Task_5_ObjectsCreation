package Chapter3;

    class Dog {
        String name;
        String says;

        Dog(String name, String says) {
            this.name = name;
            this.says = says;
        }
    }
    public class Task_5_ObjectsCreation {

        public static void main (String[] args) {
            Dog dog1 = new Dog("spot", "wau-wau");
            Dog dog2 = new Dog("scruffy", "tyaf-tyaf");

            System.out.println("Dog with name " + dog1.name +  " says " + dog1.says);
            System.out.println("Dog with name " + dog2.name +  " says " + dog2.says);
        }
    }
